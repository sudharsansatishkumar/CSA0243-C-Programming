#include <stdio.h>
#include <math.h>
int main()
{
    printf("enter the factorial num:");
    printf("Fatorial of a given number is: %d",fact());
    return 0;
}
 int fact()
 { 
    int i,fact=1,num;
    scanf("%d",&num);
    for(i=1;i<=num;i++)
    {
        fact=fact*i;
    }
    return fact;
 }    
